#include <stdio.h>
#include "stdafx.h"
#include <math.h>
int main()
{
	int d, x1,x2,y1, y2,x3,y3;
	printf("Vvedite koordinati x1\n");
	scanf("%d", &x1);
	printf("Vvedite koordinati x2\n");
	scanf("%d", &x2);
	printf("Vvedite koordinati y1\n");
	scanf("%d", &y1);
	printf("Vvedite koordinati y2\n");
	scanf("%d", &y2);
	d = x1 - x2;
	scanf("%d", &x3);
	d = y1 - y2;
	scanf("%d", &y3);
	d = sqrt((pow (x3,2)+(pow(y3,2))));
		printf("Dlina ravna:%d\n", d);
	scanf("%d,&d");
	return 0;

}
